{
  "name": "test-app",
  "type": "module",
  "private": true,
  "dependencies": {
    "@pikku/core": "^0.9.7",
    "@pikku/jose": "^0.9.3",
    "@pikku/schema-cfworker": "^0.9.2",
    "eventsource": "^4.0.0",
    "@pikku/cloudflare": "^0.9.2"
  },
  "devDependencies": {
    "@pikku/cli": "^0.9.7",
    "@pikku/fetch": "^0.9.2",
    "@pikku/websocket": "^0.9.2",
    "@types/node": "^22",
    "concurrently": "^9.2.0",
    "typescript": "^5.5.2",
    "@cloudflare/workers-types": "^4.20241218.0",
    "wrangler": "^4.6.0"
  },
  "scripts": {
    "pikku": "pikku all",
    "test:template": "bash run-tests.sh --url http://localhost:8787",
    "test:http": "npx tsx client/http-fetch.ts",
    "test:sse": "npx tsx client/http-sse.ts",
    "test:rpc": "npx tsx client/rpc.ts",
    "test:websocket": "npx tsx client/websocket.ts",
    "deploy": "wrangler deploy",
    "start": "wrangler dev",
    "postinstall": "pikku all",
    "test": "npm run test:http",
    "stackblitz": "concurrently 'pikku --watch --silent' 'npm run dev'"
  },
  "stackblitz": {
    "startCommand": "npm run stackblitz"
  }
}